# ============================================================================
# ADK Multi-Agent Framework - Environment Configuration
# ============================================================================
# Copy this file to .env and fill in your actual values
# Command: cp .env.example .env
# ============================================================================

# ----------------------------------------------------------------------------
# REQUIRED: Google Cloud / Vertex AI Configuration
# ----------------------------------------------------------------------------
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=AIzaSyDDuV_pA3cobMj6fsgK7iz7FhcQ0e2qwp4

# Your Google Cloud Project ID (e.g., "my-project-123")
# Find it at: https://console.cloud.google.com/
GOOGLE_CLOUD_PROJECT=test-agentic-project-12345

# Google Cloud Region (default: us-central1)
GOOGLE_CLOUD_REGION=us-central1

# ----------------------------------------------------------------------------
# OPTIONAL: Application Configuration
# ----------------------------------------------------------------------------
# Environment: development, staging, production
ENVIRONMENT=development

# Default AI Model
DEFAULT_MODEL=gemini-2.0-flash-exp

# Logging
LOG_LEVEL=DEBUG

# ----------------------------------------------------------------------------
# OPTIONAL: Redis Configuration
# ----------------------------------------------------------------------------
# Redis connection URL
# For Docker: redis://redis:6379
# For local: redis://localhost:6379
REDIS_URL=redis://redis:6379

# Session TTL in seconds (default: 3600 = 1 hour)
REDIS_SESSION_TTL=3600

# ----------------------------------------------------------------------------
# OPTIONAL: Multi-Tenancy
# ----------------------------------------------------------------------------
# Default tenant ID for requests without tenant_id
DEFAULT_TENANT_ID=default

# ----------------------------------------------------------------------------
# OPTIONAL: Rate Limiting
# ----------------------------------------------------------------------------
# Requests per minute per tenant
RATE_LIMIT_PER_MINUTE=60

# ----------------------------------------------------------------------------
# OPTIONAL: Vertex AI Memory Bank (Phase 4)
# ----------------------------------------------------------------------------
# Enable Vertex AI Memory Bank (false = use Redis)
VERTEX_MEMORY_ENABLED=false

# Memory collection name
VERTEX_MEMORY_COLLECTION=agent_memories

# ----------------------------------------------------------------------------
# OPTIONAL: CORS Configuration
# ----------------------------------------------------------------------------
# Allowed origins (JSON array format)
# For development: ["*"] allows all
# For production: ["https://example.com", "https://app.example.com"]
CORS_ORIGINS=["*"]

# ----------------------------------------------------------------------------
# OPTIONAL: Feature Flags
# ----------------------------------------------------------------------------
# Enable Prometheus metrics
ENABLE_METRICS=true

# Enable Cloud Trace
ENABLE_TRACING=false

# Enable audit logging
ENABLE_AUDIT_LOG=false

# ----------------------------------------------------------------------------
# OPTIONAL: Security & Authentication
# ----------------------------------------------------------------------------
# JWT Secret Key (CHANGE THIS IN PRODUCTION!)
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=your-secret-key-change-in-production-min-32-chars-please-use-secure-random-key

# JWT token expiration (in minutes)
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Require API key or JWT authentication (set to true in production)
REQUIRE_API_KEY=false

# API Keys for service-to-service authentication (comma-separated)
# Example: API_KEYS=key1,key2,key3
API_KEYS=

